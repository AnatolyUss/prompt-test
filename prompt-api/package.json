  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "format": "prettier . --ignore-path ./.prettierignore --write && git status",
    "fb": "npm run format && npm run build",
    "flb": "npm run format && npm run lint && npm run build",
    "nest": "nest",
    "start": "node dist/src/main.js",
    "start:prod": "node dist/src/main.js",
    "start:dev": "nest start",
    "start:debug": "nest start --debug",
    "test": "jest --testPathPattern=dist/test --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=dist/test/unit --detectOpenHandles --forceExit",
    "test:integration": "jest --testPathPattern=dist/test/integration --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnatolyUss/prompt-test.git"
  },
  "engines": {
    "node": ">=20.15.1"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.3",
    "@nestjs/common": "10.3.10",
    "@nestjs/core": "10.3.10",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/swagger": "7.4.0",
    "aws-sdk": "2.1691.0",
    "axios": "1.7.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "helmet": "7.1.0",
    "pdf-parse": "1.1.1",
    "redis": "4.7.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.24.8",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.3.10",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "1.4.12",
    "@types/node": "20.14.11",
    "@types/pdf-parse": "1.1.4",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
